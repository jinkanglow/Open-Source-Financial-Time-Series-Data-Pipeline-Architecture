{
  "dashboard": {
    "title": "Financial Pipeline Health Dashboard",
    "tags": ["financial", "pipeline", "health", "monitoring"],
    "timezone": "browser",
    "schemaVersion": 27,
    "version": 1,
    "refresh": "10s",
    "panels": [
      {
        "id": 1,
        "title": "Overall System Status",
        "type": "stat",
        "targets": [
          {
            "expr": "pipeline_overall_status",
            "legendFormat": "System Health"
          }
        ],
        "thresholds": {
          "mode": "absolute",
          "steps": [
            {"value": 0, "color": "red"},
            {"value": 0.5, "color": "yellow"},
            {"value": 1, "color": "green"}
          ]
        },
        "gridPos": {"h": 4, "w": 6, "x": 0, "y": 0}
      },
      {
        "id": 2,
        "title": "Kafka Consumer Lag",
        "type": "graph",
        "targets": [
          {
            "expr": "pipeline_kafka_consumer_lag",
            "legendFormat": "Consumer Lag (records)"
          }
        ],
        "yaxes": [
          {
            "format": "short",
            "label": "Records"
          }
        ],
        "thresholds": [
          {
            "value": 1000,
            "colorMode": "critical",
            "op": "gt"
          },
          {
            "value": 5000,
            "colorMode": "warning",
            "op": "gt"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 4}
      },
      {
        "id": 3,
        "title": "TimescaleDB Continuous Aggregate Freshness",
        "type": "graph",
        "targets": [
          {
            "expr": "pipeline_timescaledb_continuous_aggregate_lag_seconds",
            "legendFormat": "Lag (seconds)"
          }
        ],
        "yaxes": [
          {
            "format": "s",
            "label": "Lag"
          }
        ],
        "thresholds": [
          {
            "value": 120,
            "colorMode": "critical",
            "op": "gt"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 4}
      },
      {
        "id": 4,
        "title": "Model PnL Comparison (Shadow vs Canary)",
        "type": "graph",
        "targets": [
          {
            "expr": "pipeline_model_performance_pnl_diff_percent",
            "legendFormat": "PnL Difference (%)"
          },
          {
            "expr": "pipeline_model_performance_canary_pnl_usd",
            "legendFormat": "Canary PnL (USD)"
          }
        ],
        "yaxes": [
          {
            "format": "currencyUSD",
            "label": "PnL"
          }
        ],
        "thresholds": [
          {
            "value": 10,
            "colorMode": "warning",
            "op": "gt"
          },
          {
            "value": 20,
            "colorMode": "critical",
            "op": "gt"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 12}
      },
      {
        "id": 5,
        "title": "Data Quality Score by Feature",
        "type": "graph",
        "targets": [
          {
            "expr": "pipeline_feast_features_data_quality_score",
            "legendFormat": "{{feature}}"
          }
        ],
        "yaxes": [
          {
            "format": "percentunit",
            "label": "Quality Score",
            "min": 0,
            "max": 1
          }
        ],
        "thresholds": [
          {
            "value": 0.85,
            "colorMode": "critical",
            "op": "lt"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 12}
      },
      {
        "id": 6,
        "title": "Component Health Status",
        "type": "table",
        "targets": [
          {
            "expr": "pipeline_kafka_status",
            "format": "table"
          },
          {
            "expr": "pipeline_timescaledb_status",
            "format": "table"
          },
          {
            "expr": "pipeline_flink_jobs_status",
            "format": "table"
          },
          {
            "expr": "pipeline_feast_features_status",
            "format": "table"
          }
        ],
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 20}
      },
      {
        "id": 7,
        "title": "Flink Checkpoint Success Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "pipeline_flink_jobs_checkpoint_success_rate",
            "legendFormat": "{{job}}"
          }
        ],
        "yaxes": [
          {
            "format": "percentunit",
            "label": "Success Rate",
            "min": 0.95,
            "max": 1
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 28}
      },
      {
        "id": 8,
        "title": "Feature Staleness",
        "type": "graph",
        "targets": [
          {
            "expr": "pipeline_feature_quality_staleness_minutes",
            "legendFormat": "{{feature}}"
          }
        ],
        "yaxes": [
          {
            "format": "m",
            "label": "Staleness"
          }
        ],
        "thresholds": [
          {
            "value": 5,
            "colorMode": "warning",
            "op": "gt"
          },
          {
            "value": 30,
            "colorMode": "critical",
            "op": "gt"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 28}
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "annotations": {
      "list": [
        {
          "name": "Critical Alerts",
          "datasource": "Prometheus",
          "enable": true,
          "expr": "pipeline_overall_status == 0",
          "iconColor": "red"
        },
        {
          "name": "Quality Threshold Violations",
          "datasource": "Prometheus",
          "enable": true,
          "expr": "pipeline_feast_features_data_quality_score < 0.85",
          "iconColor": "orange"
        }
      ]
    }
  }
}
